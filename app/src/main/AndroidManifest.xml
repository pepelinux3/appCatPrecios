<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adrprecios">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">

        <activity android:name="adr.precios.view.PriceFullActivity" />
        <activity android:name="adr.precios.view.ImagenActivity" />
        <activity android:name="adr.precios.view.PriceActivity"
            android:configChanges="orientation|screenSize"/>
        <activity android:name="adr.precios.view.GroupActivity" />
        <activity android:name="adr.precios.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>